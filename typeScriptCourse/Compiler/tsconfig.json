{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "noImplicitAny": true,
    "sourceMap": true, //true to enable source ts file being sent to the browser, good to debug
    "noEmitOnError": true, //does not compile if error in typeScript,
    "strictNullChecks": true,
    "noUnusedParameters": true
  },
  "exclude": [
    "node_modules"
  ]
}